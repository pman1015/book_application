<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER_LEFT" onMouseClicked="#loadCustomer" prefHeight="56.0" prefWidth="690.0" spacing="48.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.customer_book.Pages.CustomersListPage.CustomerListCardController">
   <children>
      <Label fx:id="CustomerName" prefHeight="32.0" prefWidth="260.0" text="Label">
         <styleClass>
            <String fx:value="mediumFont" />
            <String fx:value="darkText" />
         </styleClass>
      </Label>
      <Label fx:id="CustomerPhoneNumber" prefHeight="32.0" prefWidth="234.0" text="Label">
         <styleClass>
            <String fx:value="mediumFont" />
            <String fx:value="darkText" />
         </styleClass>
      </Label>
      <Label fx:id="CustomerNickName" prefHeight="32.0" prefWidth="233.0" text="Label">
         <styleClass>
            <String fx:value="mediumFont" />
            <String fx:value="darkText" />
         </styleClass>
      </Label>
      <Label fx:id="CustomerRating" prefHeight="32.0" prefWidth="222.0" text="Label">
         <styleClass>
            <String fx:value="mediumFont" />
            <String fx:value="darkText" />
         </styleClass>
      </Label>
   </children>
   <padding>
      <Insets bottom="12.0" left="8.0" right="8.0" top="12.0" />
   </padding>
   <styleClass>
      <String fx:value="roundedCorners" />
      <String fx:value="card" />
   </styleClass>
</HBox>
